set adjust-open best-fit
set scroll-step 100
set window-height 900
set window-width 1440

map = zoom default

map <PageUp> scroll half-up
map <PageDown> scroll half-down

map <Space> navigate next
map , navigate previous
map . navigate next

map <A-1> set "pages-per-row 1"
map <A-2> set "pages-per-row 2"
